{
  "manifest_version": 2,
  "name": "FitTime",
  "version": "1.0",
  "description": "Gamify your exercise by converting it into screentime!",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
